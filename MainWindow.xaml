<Window x:Class="TimeCalc.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TimeCalc" Height="460.02" Width="222.851" ResizeMode="NoResize" >
	<Grid Margin="0,0,2,-19" Background="AliceBlue" Height="432" VerticalAlignment="Top" >
		<TextBox HorizontalAlignment="Left" Height="311" Margin="10,10,0,0" Name="Times"
                 AcceptsReturn="True"
                 TextWrapping="Wrap" VerticalAlignment="Top" Width="186" RenderTransformOrigin="0.5,0.5" FontSize="14">
		</TextBox>
		<Button Content="Total" HorizontalAlignment="Left" Margin="10,329,0,0" VerticalAlignment="Top" Width="75" Click="Total_Click"/>
		<Label Name="TotalSoFar" Content="0:00" HorizontalAlignment="Left" Margin="109,326,0,0" VerticalAlignment="Top" Width="87" RenderTransformOrigin="0.506,-0.038"/>
		<Label Content="Target" HorizontalAlignment="Left" Margin="10,354,0,0" VerticalAlignment="Top"/>
		<Label Content="Time To Go" HorizontalAlignment="Left" Margin="10,385,0,0" VerticalAlignment="Top"/>
		<Label Name="TTG" Content="0:00" HorizontalAlignment="Left" Margin="109,385,0,0" VerticalAlignment="Top"/>
		<TextBox Name="Target" HorizontalAlignment="Left" Height="23" Margin="109,358,0,0" TextWrapping="Wrap" Text="00:00" VerticalAlignment="Top" Width="87"/>

	</Grid>
</Window>
